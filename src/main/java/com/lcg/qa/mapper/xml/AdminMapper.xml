<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lcg.qa.mapper.AdminMapper">

    <select id="findPasswordByAdmin" parameterType="String" resultType="String">
        select password from admin where name = #{name} limit 1;
    </select>
</mapper>